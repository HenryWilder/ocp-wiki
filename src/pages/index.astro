---
import BaseLayout from '../layouts/BaseLayout.astro';
import ArticleLayout from '../layouts/ArticleLayout.astro';
import OCPListEntry from '../components/OCPListEntry.astro';
import { EntryVisibility } from '../components/OCPListEntry.astro';
import objects from '../../public/data/objects.json'
export interface ObjectEntry {
	id: string,
	path: string,
	name: string,
	title: string,
	notes?: string|string[]
}
const objectList: ObjectEntry[] = objects;
---

<ArticleLayout url="./index" title="Main" name="Anomaly List" desc="Obscure, Classify, Preserve">
<ul>
	{objectList.map((item) => <OCPListEntry visibility={EntryVisibility.Normal} name={item.name} title={item.title} path={item.path} />)}
</ul>
</ArticleLayout>
