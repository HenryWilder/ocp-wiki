---
import ArticleLayout from '../layouts/ArticleLayout.astro';
import OCPListEntry from '../components/OCPListEntry.astro';
import objects from '../../public/data/objects.json';
import type { IObjectEntry } from '../scripts/interfaces';
import AuthorCard from '../components/AuthorCard.astro';
import type { IAuthor } from '../scripts/interfaces';
import authors from '../../public/data/authors.json';
const authorList: IAuthor[] = authors;
const objectList: IObjectEntry[] = objects;
---

<ArticleLayout url="author-list" title="Authors" name="Author List" desc="Obscure, Classify, Preserve">
<ul>
	{authorList.map(author => <AuthorCard authorData={author} />)}
</ul>
</ArticleLayout>
