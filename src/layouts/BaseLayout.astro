---
import Meta from '../components/Meta.astro';
import SiteHeader from '../components/SiteHeader.astro'
interface Props {
  url: string;
  title: string;
  desc: string;
}
const { url, title, desc } = Astro.props
---
<html lang="en">
  <head>
    <Meta title={title} url={url} desc={desc} />
  </head>
  <body>
    <SiteHeader />
    <div id="content-viewport">
      <slot />
    </div>
  </body>
</html>