---
import TestLog from "../components/reports/TestLog.astro";
import Redacted from "../components/typography/Redacted.astro";
import RedactedDate from "../components/typography/RedactedDate.astro";
import type { ITestLog } from '../scripts/index';
const item: ITestLog = {
    objectNumber: '000',
    instance: '1'
};
---
<TestLog item={item}>
    <Fragment slot="date"><RedactedDate/></Fragment>
    <Fragment slot="time"><Redacted>####</Redacted>hrs</Fragment>
    <Fragment slot="location"><Redacted>***********</Redacted></Fragment>

    <Fragment slot="researcher">Jr RSR <Redacted>*******</Redacted></Fragment>
    <Fragment slot="object">000</Fragment>
    <Fragment slot="approver">n/a</Fragment>

    <Fragment slot="goal">Study the object.</Fragment>
    <Fragment slot="procedure">Get a piece of paper and write down what the object does.</Fragment>

    <Fragment slot="results">Failed.</Fragment>
    <Fragment slot="comments">It ate my paper :c</Fragment>
</TestLog>